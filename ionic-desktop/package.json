{
  "name": "ionic-desktop",
  "version": "0.0.2",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "main": "src/electron-starter.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dist": "./node_modules/.bin/electron-builder --ia32 --x64 --win --linux",
    "electron-serve": "ng build --prod && electron .",
    "mac-release": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icon.icns --prune=true --out=release-builds",
    "windows-release": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Electron Tutorial App'",
    "linux-release-x64": "./node_modules/.bin/electron-packager . electron-tutorial-app --overwrite --asar --platform=linux --arch=x64 --icon='src/assets/icon.png' --prune=true --out=release-builds",
    "linux-release-i386": "./node_modules/.bin/electron-packager . electron-tutorial-app --overwrite --asar --platform=linux --arch=i386 --icon='resources/icon.png' --prune=true --out=release-builds",
    "linux-build-x64": "npm run linux-release-x64 && ./node_modules/.bin/electron-installer-debian --arch x64 --src ./release-builds/electron-tutorial-app-linux-x64 --dest builds/ionic-desktop-linux",
    "linux-build-i386": "npm run linux-release-i386 && ./node_modules/.bin/electron-installer-debian --arch ia32 --src ./release-builds/electron-tutorial-app-win32-ia32 --dest builds/ionic-desktop-linux",
    "linux-release": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icon.png --prune=true --out=release-builds",
    "windows-release-ia32": "./node_modules/.bin/electron-packager . electron-tutorial-app --overwrite --asar --platform=win32 --arch=ia32 --icon=\"src/assets/icon.png\" --prune=true --out=release-builds --version-string.CompanyName=BrightSoftwares --version-string.FileDescription=CE --version-string.ProductName='Application Desktop Tutorial'",
    "windows-build-ia32": "npm run windows-release-ia32 && ./node_modules/.bin/electron-installer-windows --arch ia32 --src ./release-builds/electron-tutorial-app-win32-ia32 --dest builds/ionic-desktop-windows",
    "mac-build": "electron-installer-dmg ./release-builds/ionic-desktop-darwin-x64/ionic-desktop.app builds/ionic-desktop"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~7.1.4",
    "@angular/core": "~14.1.0",
    "@angular/forms": "~7.1.4",
    "@angular/http": "~7.2.16",
    "@angular/platform-browser": "~7.1.4",
    "@angular/platform-browser-dynamic": "~7.1.4",
    "@angular/router": "~7.1.4",
    "@ionic-native/core": "5.0.0-beta.21",
    "@ionic-native/splash-screen": "5.0.0-beta.21",
    "@ionic-native/status-bar": "5.0.0-beta.21",
    "@ionic/angular": "4.0.0-rc.0",
    "core-js": "^3.24.0",
    "rxjs": "~6.3.3",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1401.0",
    "@angular-devkit/build-angular": "~0.11.4",
    "@angular-devkit/core": "~14.1.0",
    "@angular-devkit/schematics": "~14.1.2",
    "@angular/cli": "~15.1.4",
    "@angular/compiler": "~14.1.0",
    "@angular/compiler-cli": "~7.1.4",
    "@angular/language-service": "~14.1.0",
    "@ionic/angular-toolkit": "~1.2.0",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.0",
    "codelyzer": "~4.5.0",
    "electron": "^20.0.2",
    "electron-builder": "^20.38.4",
    "electron-installer-debian": "^1.0.1",
    "electron-installer-dmg": "^2.0.0",
    "electron-installer-windows": "^1.1.0",
    "electron-packager": "^13.0.1",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~10.9.1",
    "tslint": "~5.20.1",
    "typescript": "~4.7.4"
  },
  "description": "An Ionic project"
}
